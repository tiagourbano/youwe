    "require-dev": {
        "phpunit/phpunit": "5.7",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "authors": [
        {
            "name": "Tiago Urbano",
            "email": "tiago.urbano@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "Bootstrap\\": "Bootstrap/"
